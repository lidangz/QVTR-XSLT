<?xml version="1.0" encoding="UTF-8"?>
<SimpleRDBMS:Schema xmlns:SimpleRDBMS="urn:SimpleRDBMS.ecore" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="myPackage">
	<table name="Library">
		<column name="Library_tid" type="NUMBER" rkey="Library_pk"/>
		<rkey name="Library_pk" column="Library_tid"/>
		<foreignKey name="Library_LibraryToMedium_Medium" refersTo="Medium"/>
	</table>
	<table name="Medium">
		<column name="Medium_tid" type="NUMBER" rkey="Medium_pk"/>
		<rkey name="Medium_pk" column="Medium_tid"/>
		<column name="Medium_author_name" type="VARCHAR"/>
	</table>
	<table name="DVD">
		<column name="DVD_tid" type="NUMBER" rkey="DVD_pk"/>
		<rkey name="DVD_pk" column="DVD_tid"/>
		<column name="DVD_author_name" type="VARCHAR"/>
	</table>
	<table name="Book">
		<column name="Book_tid" type="NUMBER" rkey="Book_pk"/>
		<rkey name="Book_pk" column="Book_tid"/>
		<column name="Book_numberOfPages" type="NUMBER"/>
		<column name="Book_author_name" type="VARCHAR"/>
	</table>
	<table name="Author">
		<column name="Author_tid" type="NUMBER" rkey="Author_pk"/>
		<rkey name="Author_pk" column="Author_tid"/>
		<column name="Author_name" type="VARCHAR"/>
	</table>
</SimpleRDBMS:Schema>